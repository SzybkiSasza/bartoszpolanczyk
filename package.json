{
  "name": "bartoszpolanczyk",
  "version": "0.0.1",
  "description": "My own website",
  "homepage": "https://bartoszpolanczyk.com",
  "license": "MIT",
  "author": "Bartek Polanczyk <bartosz.polanczyk@gmail.com>",
  "main": "index.js",
  "repository": "git@github.com:SzybkiSasza/bartoszpolanczyk.git",
  "scripts": {
    "build": "rm -rf ./dist && yarn build-server && yarn build-client",
    "build-client": "node scripts/build.js",
    "build-server": "webpack --config config/webpack-server.conf.js",
    "heroku-postbuild": "yarn build",
    "lint": "yarn lint:server && yarn lint:client && yarn lint:js && yarn lint:css",
    "lint:client": "tslint -p config/tsconfig-client.json -c tslint.json",
    "lint:server": "tslint -p config/tsconfig-server.json -c tslint.json  ",
    "lint:js": "eslint config src scripts --fix",
    "lint:css": "stylelint src/**/*.css",
    "start": "node dist/server/server.js",
    "start:client": "node scripts/start.js",
    "test": "jest"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression": "^1.7.2",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.19.2",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "lusca": "^1.6.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/lusca": "^1.5.0",
    "@types/node": "^9.0.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-jsx": "^0.9.0",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "tsconfig-paths-webpack-plugin": "^3.1.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "engines": {
    "node": "8.10.0",
    "yarn": "1.6.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ]
  }
}
